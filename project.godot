; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Friday Night Funkin': NULL Engine"
config/version="1.1.0"
config/tags=PackedStringArray("2d", "funkin", "rhythm")
run/main_scene="res://Init.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="NULLSonic/NULLEngine"
config/features=PackedStringArray("4.4", "GL Compatibility")
boot_splash/bg_color=Color(1, 0.141176, 1, 1)
boot_splash/image="uid://ceuewex8spqfl"
config/icon="res://assets/images/default_mod_icon.png"

[autoload]

SaveData="*res://autoload/scripts/SaveData.gd"
SettingsAPI="*res://autoload/scripts/SettingsAPI.gd"
DiscordRPCLoader="*res://addons/discord-rpc-gd/nodes/discord_autoload.gd"
GameJolt="*res://autoload/scenes/GameJoltAPI.tscn"
ModManager="*res://autoload/scripts/ModManager.gd"
EngineTheme="*res://autoload/scripts/EngineTheme.gd"
TrophyHandler="*res://autoload/scenes/TrophyDisplay.tscn"
HUDHandler="*res://autoload/scripts/HUDHandler.gd"
Global="*res://autoload/scripts/Global.gd"
Conductor="*res://autoload/scripts/Conductor.gd"
Scoring="*res://autoload/scripts/Scoring.gd"
Ranking="*res://autoload/scripts/Ranking.gd"
Constants="*res://autoload/scripts/Constants.gd"
Audio="*res://autoload/scenes/Audio.tscn"
ScreenshotHandler="*res://autoload/scenes/ScreenshotHandler.tscn"
Transition="*res://autoload/scenes/Transition.tscn"
VolumeSlider="*res://autoload/scenes/VolumeSlider.tscn"
FPS="*res://autoload/scenes/FPS.tscn"
HighScore="*res://autoload/scripts/HighScore.gd"
RichPresence="*res://autoload/scripts/DiscordRPC.gd"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/stretch/mode="canvas_items"
window/vsync/vsync_mode=0
mouse_cursor/custom_image="res://assets/images/cursor/cursor-default.png"

[editor_plugins]

enabled=PackedStringArray("res://addons/SimpleTODO/plugin.cfg", "res://addons/discord-rpc-gd/plugin.cfg", "res://addons/gamejolt/plugin.cfg", "res://addons/gdanimate/plugin.cfg")

[engine]

customization/check_for_updates=false
customization/chart_editor_on_release=false
customization/first_launch_menu=true
customization/hud_editor=true
customization/chart_type="FunkinVSlice"

[gui]

common/snap_controls_to_pixels=false

[importer_defaults]

texture_atlas={
"atlas_file": "",
"crop_to_region": false,
"import_mode": 0,
"trim_alpha_border_from_region": true
}

